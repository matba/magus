<?xml version="1.0"?>
<rdf:RDF
    xmlns:profile="http://www.daml.org/services/owl-s/1.2/Profile.owl#"
    xmlns:service="http://www.daml.org/services/owl-s/1.2/Service.owl#"
    xmlns:process="http://www.daml.org/services/owl-s/1.2/Process.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:grounding="http://www.daml.org/services/owl-s/1.2/Grounding.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
	xmlns:expr="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#"
	xmlns:swrl="http://www.w3.org/2003/11/swrl#"
  xml:base="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Grounding.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Process.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Service.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Profile.owl"/>
	<owl:imports rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml"/>
  </owl:Ontology>
  <service:Service rdf:ID="RequestProductionSchedule">
    <service:describedBy  rdf:ID="RequestProductionScheduleProcess1">
      <process:AtomicProcess rdf:ID="RequestProductionScheduleProcess">
        <process:hasInput>
          <process:Input rdf:ID="purchaseOrder">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >purchaseOrder</rdfs:label>
            <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
            >http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#PurchaseOrder</process:parameterType>
          </process:Input>
        </process:hasInput>
        <service:describes rdf:resource="#RequestProductionSchedule"/>

		 <process:hasPrecondition>
			  <expr:SWRL-Condition rdf:ID="RequestProductionScheduleServicePrec">
				<expr:expressionObject>
				  <swrl:AtomList>
					<rdf:rest>
					  <swrl:AtomList>
						<rdf:first>
						  <swrl:ClassAtom>
							<swrl:argument1>
							  <swrl:Variable rdf:ID="customerInfo"/>
							</swrl:argument1>
							<swrl:classPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#Customer"/>
						  </swrl:ClassAtom>
						</rdf:first>
						<rdf:rest>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:IndividualPropertyAtom>
								<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#InvoiceCreationRequested"/>
								<swrl:argument1 rdf:resource="#customerInfo"/>
								<swrl:argument2 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml#purchaseOrder"/>
							  </swrl:IndividualPropertyAtom>
							</rdf:first>
							<rdf:rest>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:ClassAtom>
								<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml#purchaseOrder"/>
								<swrl:classPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#PurchaseOrder"/>
							  </swrl:ClassAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						  </swrl:AtomList>
						</rdf:rest>
						  </swrl:AtomList>
						</rdf:rest>
					  </swrl:AtomList>
					</rdf:rest>
				  </swrl:AtomList>
				</expr:expressionObject>
				<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
				<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
				>Precondition for Request Shipping Service</rdfs:comment>
			  </expr:SWRL-Condition>
		 </process:hasPrecondition>

        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >RequestProductionScheduleProcess</rdfs:label>
      </process:AtomicProcess>
    </service:describedBy>
  </service:Service>
  <service:Service rdf:ID="RequestProductionScheduleCallback">
    <service:describedBy  rdf:ID="RequestProductionScheduleCallbackProcess1">
      <process:AtomicProcess rdf:ID="RequestProductionScheduleCallbackProcess">
        <service:describes rdf:resource="#RequestProductionScheduleCallback"/>
        <process:hasOutput>
		  <process:Output rdf:ID="productionSchedule">
			<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
			>schedule</rdfs:label>
			<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
			>http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#ProductionSchedule</process:parameterType>
		  </process:Output>
        </process:hasOutput>
		  <process:hasResult>
			  <expr:SWRL-Condition rdf:ID="RequestProductionScheduleCallbackServiceEff">
				<expr:expressionObject>
				  <swrl:AtomList>
					<rdf:first>
					  <swrl:ClassAtom>
						<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml#productionSchedule"/>
						<swrl:classPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#ProductionSchedule"/>
					  </swrl:ClassAtom>
					</rdf:first>
					<rdf:rest>
					  <swrl:AtomList>
						<rdf:first>
						  <swrl:IndividualPropertyAtom>
							<swrl:argument2 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml#productionSchedule"/>
							<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestProductionSchedule.xml#purchaseOrder"/>
							<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#HasProductionSchedule"/>
						  </swrl:IndividualPropertyAtom>
						</rdf:first>
						<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
					  </swrl:AtomList>
					</rdf:rest>
				  </swrl:AtomList>
				</expr:expressionObject>
				<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
				<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
				>Effect for Request Shipping Service</rdfs:comment>
			  </expr:SWRL-Condition>
		 </process:hasResult>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >RequestProductionScheduleCallbackProcess</rdfs:label>
      </process:AtomicProcess>
    </service:describedBy>

  </service:Service>
</rdf:RDF>


