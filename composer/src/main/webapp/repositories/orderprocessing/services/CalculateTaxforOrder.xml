<?xml version="1.0"?>
<rdf:RDF
    xmlns:profile="http://www.daml.org/services/owl-s/1.2/Profile.owl#"
    xmlns:service="http://www.daml.org/services/owl-s/1.2/Service.owl#"
    xmlns:process="http://www.daml.org/services/owl-s/1.2/Process.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:grounding="http://www.daml.org/services/owl-s/1.2/Grounding.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
	xmlns:expr="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#"
	xmlns:swrl="http://www.w3.org/2003/11/swrl#"
  xml:base="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Service.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Grounding.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Profile.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Process.owl"/>
    <owl:imports rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml"/>
  </owl:Ontology>
  <service:Service rdf:ID="CalculateTaxforOrder">
      <service:describedBy>
      <process:AtomicProcess rdf:ID="CalculateTaxforOrderProcess">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >CalculateTaxforOrderProcess</rdfs:label>
        <service:describes rdf:resource="#CalculateTaxforOrder"/>
        <process:hasInput >
			<process:Input rdf:ID="purchaseOrder">
				<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
				>purchaseOrder</rdfs:label>
				<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
				>http://magus.online/repositories/orderprocessing/contextModel.xml#PurchaseOrder</process:parameterType>
            </process:Input>
		</process:hasInput>
		<process:hasPrecondition>
			  <expr:SWRL-Condition rdf:ID="CalculateTaxforOrderServicePre">
				<expr:expressionObject>
				  <swrl:AtomList>
					<rdf:rest>
					  <swrl:AtomList>
						<rdf:rest>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:ClassAtom>
								<swrl:classPredicate rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml#Customer"/>
								<swrl:argument1>
								  <swrl:Variable rdf:ID="cu"/>
								</swrl:argument1>
							  </swrl:ClassAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						  </swrl:AtomList>
						</rdf:rest>
						<rdf:first>
						  <swrl:IndividualPropertyAtom>
							<swrl:argument1 rdf:resource="#cu"/>
							<swrl:propertyPredicate rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml#InvoiceCreationRequested"/>
							<swrl:argument2 rdf:resource="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml#purchaseOrder"/>
						  </swrl:IndividualPropertyAtom>
						</rdf:first>
					  </swrl:AtomList>
					</rdf:rest>
					<rdf:first>
					  <swrl:ClassAtom>
						<swrl:classPredicate rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml#PurchaseOrder"/>
						<swrl:argument1 rdf:resource="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml#purchaseOrder"/>
					  </swrl:ClassAtom>
					</rdf:first>
				  </swrl:AtomList>
				</expr:expressionObject>
				<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
				<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
				></rdfs:comment>
			  </expr:SWRL-Condition>
		 </process:hasPrecondition>
	  </process:AtomicProcess>
    </service:describedBy>
  </service:Service>
<service:Service rdf:ID="CalculateTaxforOrderCallback">
      <service:describedBy>
      <process:AtomicProcess rdf:ID="CalculateTaxforOrderCallbackProcess">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >CalculateTaxforOrderCallbackProcess</rdfs:label>
        <service:describes rdf:resource="#CalculateTaxforOrderCallback"/>
		<process:hasOutput>
		  <process:Output rdf:ID="taxInformationPart">
			<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
			>http://magus.online/repositories/orderprocessing/contextModel.xml#TaxInfo</process:parameterType>
			<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
			>taxInformationPart</rdfs:label>
		  </process:Output>
		</process:hasOutput>
		<process:hasResult>
		  <expr:SWRL-Condition rdf:ID="CalculateTaxforOrderCallbackCBEff">
			<expr:expressionObject>
					<swrl:AtomList>
						<rdf:first>
						  <swrl:ClassAtom>
							<swrl:argument1 rdf:resource="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml#taxInformationPart"/>
							<swrl:classPredicate rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml#TaxInfo"/>
						  </swrl:ClassAtom>
						</rdf:first>
						<rdf:rest>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:IndividualPropertyAtom>
								<swrl:argument2 rdf:resource="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml#taxInformationPart"/>
								<swrl:propertyPredicate rdf:resource="http://magus.online/repositories/orderprocessing/contextModel.xml#HasTaxInfo"/>
								<swrl:argument1 rdf:resource="http://magus.online/repositories/orderprocessing/services/CalculateTaxforOrder.xml#purchaseOrder"/>
							  </swrl:IndividualPropertyAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						  </swrl:AtomList>
						</rdf:rest>
					</swrl:AtomList>
					</expr:expressionObject>
		  </expr:SWRL-Condition>
		</process:hasResult>
	  </process:AtomicProcess>
    </service:describedBy>
    
  </service:Service>
</rdf:RDF>
