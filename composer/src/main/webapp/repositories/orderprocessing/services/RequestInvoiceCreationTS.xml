<?xml version="1.0"?>
<rdf:RDF
    xmlns:profile="http://www.daml.org/services/owl-s/1.2/Profile.owl#"
    xmlns:service="http://www.daml.org/services/owl-s/1.2/Service.owl#"
    xmlns:process="http://www.daml.org/services/owl-s/1.2/Process.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:grounding="http://www.daml.org/services/owl-s/1.2/Grounding.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
    xmlns:expr="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
  xml:base="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Profile.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Service.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Process.owl"/>
    <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.2/Grounding.owl"/>
    <owl:imports rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml"/>
  </owl:Ontology>
  <service:Service rdf:ID="RequestInvoiceCreationTS">
    <service:describedBy >
	 <process:AtomicProcess rdf:ID="RequestInvoiceCreationTSProcess">
                <service:describes rdf:resource="#RequestInvoiceCreationTS"/>
                <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RequestInvoiceCreationTSProcess</rdfs:label>
				<process:hasInput>
				  <process:Input rdf:ID="customerInfo">
					<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
					>http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#Customer</process:parameterType>
					<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
					>customerInfo</rdfs:label>
				  </process:Input>
				</process:hasInput>
				<process:hasInput>
				  <process:Input rdf:ID="purchaseOrder">
					<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
					>http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#PurchaseOrder</process:parameterType>
					<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
					>purchaseOrder</rdfs:label>
				  </process:Input>
				</process:hasInput>
				<process:hasPrecondition>
					  <expr:SWRL-Condition rdf:ID="RequestInvoiceCreationTSServicePrec">
						<expr:expressionObject>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:IndividualPropertyAtom>
								<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#Ordered"/>
								<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#customerInfo"/>
								<swrl:argument2 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#purchaseOrder"/>
							  </swrl:IndividualPropertyAtom>
							</rdf:first>
							<rdf:rest >
								<swrl:AtomList>
									<rdf:first>
									  <swrl:IndividualPropertyAtom>
										<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#!InvoiceCreationRequested"/>
										<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#customerInfo"/>
										<swrl:argument2 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#purchaseOrder"/>
									  </swrl:IndividualPropertyAtom>
									</rdf:first>
									<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						  		</swrl:AtomList>
							</rdf:rest>
						  </swrl:AtomList>
						</expr:expressionObject>
						<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
						<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
						></rdfs:comment>
					  </expr:SWRL-Condition>
				 </process:hasPrecondition>
				 <process:hasResult>
					  <expr:SWRL-Condition rdf:ID="RequestInvoiceCreationTSServicePrec">
						<expr:expressionObject>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:IndividualPropertyAtom>
								<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#InvoiceCreationRequested"/>
								<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#customerInfo"/>
								<swrl:argument2 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#purchaseOrder"/>
							  </swrl:IndividualPropertyAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						  </swrl:AtomList>
						</expr:expressionObject>
						<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
						<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
						></rdfs:comment>
					  </expr:SWRL-Condition>
				 </process:hasResult>
              </process:AtomicProcess>
	 </service:describedBy>
  </service:Service>
  <service:Service rdf:ID="RequestInvoiceCreationTSCallback">
        <service:describedBy>
          <process:AtomicProcess rdf:ID="RequestInvoiceCreationTSCallbackProcess">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >RequestInvoiceCreationTSCallbackProcess</rdfs:label>
            <service:describes rdf:resource="#RequestInvoiceCreationTSCallback"/>
            <process:hasOutput>
              <process:Output rdf:ID="IVC">
                <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >IVC</rdfs:label>
                <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
                >http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#Invoice</process:parameterType>
              </process:Output>
            </process:hasOutput>
			 <process:hasPrecondition>
				  <expr:SWRL-Condition rdf:ID="RequestInvoiceCreationTSCallbackServicePrec">
					<expr:expressionObject>

					  <swrl:AtomList>
						<rdf:rest>
						  <swrl:AtomList>
							<rdf:first>
							  <swrl:ClassAtom>
								<swrl:argument1>
								  <swrl:Variable rdf:ID="x"/>
								</swrl:argument1>
								<swrl:classPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#TaxInfo"/>
							  </swrl:ClassAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						</swrl:AtomList>
						</rdf:rest>
						<rdf:first>
						  <swrl:IndividualPropertyAtom>
							<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#TaxInfoSentToInvoicing"/>
							<swrl:argument2 rdf:resource="#x"/>

							<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#purchaseOrder"/>
						  </swrl:IndividualPropertyAtom>
						</rdf:first>
					  </swrl:AtomList>

					</expr:expressionObject>
					<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
					<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
					></rdfs:comment>
				  </expr:SWRL-Condition>
			 </process:hasPrecondition>
			 <process:hasResult>
				  <expr:SWRL-Condition rdf:ID="RequestInvoiceCreationTSCallbackServiceEff">
					<expr:expressionObject>

						  <swrl:AtomList>
							<rdf:rest>

								  <swrl:AtomList>
									<rdf:first>
									  <swrl:IndividualPropertyAtom>
										<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#IncludesTaxInfo"/>
										<swrl:argument2 rdf:resource="#x"/>
										<swrl:argument1 rdf:resource="#IVC"/>
									  </swrl:IndividualPropertyAtom>
									</rdf:first>
									<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
								  </swrl:AtomList>

							</rdf:rest>
							<rdf:first>
							  <swrl:IndividualPropertyAtom>
								<swrl:propertyPredicate rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/contextModel.xml#HasInvoice"/>
								<swrl:argument2 rdf:resource="#IVC"/>
								<swrl:argument1 rdf:resource="http://bashari.ca/magus/repositories/orderprocessing/services/RequestInvoiceCreationTS.xml#purchaseOrder"/>
							  </swrl:IndividualPropertyAtom>
							</rdf:first>
						  </swrl:AtomList>

					</expr:expressionObject>
					<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.2/generic/Expression.owl#SWRL"/>
					<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
					></rdfs:comment>
				  </expr:SWRL-Condition>
			 </process:hasResult>
          </process:AtomicProcess>
        </service:describedBy>
   </service:Service>
</rdf:RDF>


